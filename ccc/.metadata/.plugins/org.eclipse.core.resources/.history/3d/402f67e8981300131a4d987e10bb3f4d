class PageInfo
  attr_accessor :url, :char_code, :name, :title, :link_list
  def initialize (url=nil, char_code=nil, name=nil, link_list=nil)
    @url = url
    @char_code = char_code
    @name = name
    #@title = title
    @link_list = link_list
  end
  def initialize (*args)
    @url, @char_code, @name, @link_list = args
    #@title = title
  end
end